{
  "servers":
  [
    {
      "type": "real",
      "port": "8080",
      "service": {
        "serviceIdentifier": "builtin:simple_fileserver",
        "instanceName": "TestFileServer",
        "autostart": true,
        "folderInfos":
        [
          {
            "prefixToServe": "",
            "pathOnDisk": "./test",
            "precompress": true,
            "additionalHeaders": {
              "Cache-Control": "no-cache"
            }
          }
        ],
        "redirectInfos":
        [
          {
            "from": "/",
            "to": "/test.html",
            "permanent": false
          }
        ]
      }
    }
  ]
}